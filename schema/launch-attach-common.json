  "required": [
    "program",
    "server"
  ],
  "properties": {
    "cwd": {
      "type": "string"
    },
    "env": {
      "type": "object",
      "items": {
        "type": "string"
      }
    },
    "program": {
      "type": "string"
    },
    "server": {
      "type": "string",
      "enum": [
        "qemu",
        "bmp"
      ]
    },
    "smu": {
      "type": "string",
      "enum": [
        "stlink"
      ]
    }
  }
}
